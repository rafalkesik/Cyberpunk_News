<%= form_with(url: comment_likes_path,
              scope: 'comment_like',
              method: :delete, class: 'd-inline-block') do |f| %>
    <%= f.hidden_field :liked_comment_id, value: comment.id %>
    <%= button_tag(type: 'submit', class: 'btn', name: nil) do %>
        <i class="fa-solid fa-circle-up text-highlight"></i>
    <% end %>
<% end %>
